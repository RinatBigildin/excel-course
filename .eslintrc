{
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 2017,
        "sourceType": "module"
    },
    "env": {
        "es6": true,
        "browser": true,
        "node": true
    },
    "rules": {
        "semi":"off",
        "require-jsdoc" : 0,
        "linebreak-style": 0,
        "operator-linebreak":"off"
    },
    "extends": ["eslint:recommended", "google"]
}
